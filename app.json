{
  "name": "Gost",
  "description": "Deploy Gost to Heroku.",
  "keywords": ["Gost"],
  "env": {
    "VER": {
       "description": "Gost 版本号",
       "value": "2.8.1"
     },
    "METHOD": {
       "description": "加密算法，支持：aes-128-cfb, aes-192-cfb, aes-256-cfb, bf-cfb, cast5-cfb, des-cfb, rc4-md5, rc4-md5-6, chacha20, salsa20, rc4, table.",
       "value": "chacha20"
     },
     "PASSWORD": {
       "description": "用于加密传输的密码",
       "value": "ss123456"
     },
     "PORT": {
       "description": "设置监听端口，该地址是客户端需要连接的端口",
       "value": "8100"
     }
  },
  "stack": "container",
  "website": "https://github.com/wizos/gost-heroku",
  "repository": "https://github.com/wizos/gost-heroku"
 }
